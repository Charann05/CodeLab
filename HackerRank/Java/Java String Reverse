import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        int f = 0;
        int l = A.length()-1;
        
        while(f<l){
            if(A.charAt(f)!=A.charAt(l)){
                System.out.println("No");
                System.exit(0);
            }
            f++;
            l--;
        }
        System.out.println("Yes");
        
    }
}



